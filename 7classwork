    {
      "cell_type": "code",
      "source": [
        "# Convert 'yes'/'no' columns to 1/0\n",
        "yes_no_cols = ['guestroom', 'basement', 'hotwaterheating', 'airconditioning', 'prefarea']\n",
        "for col in yes_no_cols:\n",
        "    df[col] = df[col].map({'yes': 1, 'no': 0})\n",
        "\n",
        "# Convert 'furnishingstatus' to numerical labels for the target variable\n",
        "y_mapping = {'furnished': 2, 'semi-furnished': 1, 'unfurnished': 0}\n",
        "df['furnishingstatus'] = df['furnishingstatus'].map(y_mapping)\n",
        "\n",
        "# Define X (features) and y (target)\n",
        "X = df.drop('furnishingstatus', axis=1)\n",
        "y = df['furnishingstatus']\n",
        "\n",
        "print(\"X has all housing information (12 columns)\")\n",
        "print(\"y has furnishing status labels (1 column)\")\n",
        "\n",
        "# Split: 80% for training, 20% for testing\n",
        "X_train, X_test, y_train, y_test = train_test_split(\n",
        "    X, y,\n",
        "    test_size=0.2,\n",
        "    random_state=20\n",
        ")\n",
        "\n",
        "print(f\"\\nWe will train with: {len(X_train)} housing\")\n",
        "print(f\"We will test with: {len(X_test)} housing\")"
      ],
      "metadata": {
